   "source": [
    "### Type and Type Casting in Python"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "<class 'int'>\n",
      "<class 'float'>\n",
      "<class 'str'>\n"
     ]
    },
    {
     "data": {
      "text/plain": [
       "32.0"
      ]
     },
     "execution_count": 8,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "a = 31\n",
    "t = type(a) # Class <int>\n",
    "print(t)\n",
    "\n",
    "a = 31.2\n",
    "t = type(a) # Class <float>\n",
    "print(t)\n",
    "\n",
    "a = \"Noddy\"\n",
    "t = type(a) # Class <str>\n",
    "print(t)\n",
    "\n",
    "# Type Casting\n",
    "a = 31\n",
    "str(a)\n",
    "\n",
    "b = \"32\"\n",
    "int(b)\n",
    "\n",
    "c = 32\n",
    "float(c)"
   ]
  }
